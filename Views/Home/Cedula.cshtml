
@{
    ViewData["Title"] = "Cedula";
}

<h1>Cedula</h1>
<div class="justify-content-center">
    <form method="post">
        <div class="form-group">
            <label for="txtCedula">Cédula</label>
            <input type="number" class="form-control" id="data" name="data" required placeholder="402147852963">
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-success" asp-action="Result">Enviar</button>
        </div>
    </form>
</div>
